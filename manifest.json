{
  "manifest_version": 2,
  "name": "AddParam",
  "description": "A Quick way to add a parameter to the current page URL.",
  "version": "1.0.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
      "scripts": ["background.js"]
  }
}
